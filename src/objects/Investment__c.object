        <label>Series</label>
        <referenceTo>Round__c</referenceTo>
        <relationshipLabel>Investments</relationshipLabel>
        <relationshipName>Investments</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Shares1__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(ISBLANK(Shares__c),ISBLANK(Non_Voting_Shares__c)),
0,
IF(AND(ISBLANK(Shares__c),NOT(ISBLANK(Non_Voting_Shares__c))), 0 + Non_Voting_Shares__c,
 IF(AND(ISBLANK(Non_Voting_Shares__c),NOT(ISBLANK(Shares__c))), 0 + Shares__c,
 Shares__c + Non_Voting_Shares__c)
)
)</formula>
        <label># Shares (formula)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
