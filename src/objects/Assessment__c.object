        <externalId>false</externalId>
        <inlineHelpText>Rollup count of Review Items with status of Complete. Used to calculate the assessment Grade.</inlineHelpText>
        <label>Grade_Rollup</label>
        <summarizedField>review_item__c.Grade_Number__c</summarizedField>
        <summaryFilterItems>
            <field>review_item__c.Review_Item_Status__c</field>
